int x;
int a[10];

void main() {
  a[0] = 2;
  a[1] = 3;
  a[2] = 1;

  x = a[0]++;


  write("x => ", x);
  write("a[0] => ", a[0]);


  x = ++a[1];

  write("x => ", x);
  write("a[1] => ", a[1]);

  a[2] += a[2]++ + ++a[2];
  
  write("\n\n teste: ");
  write("a[2] += a[2]++ + ++a[2]");
  write("a[2] => ", a[2]);
  write("\n\n");

  a[3] = a[0];
  write("a[3] => ", a[3]);

  a[x] = 10;
  write("x => ", x);
  write("a[x] => ", a[x]);

}