int a[10];
int b[2];

void main() {
  a[0] = 2;
  a[1] = 3;
  a[2] = 1;

  a[2] += a[2]++ + ++a[2];


  a[5] = 0;

  write("a[5] => ", a[5]);
  write("a[a[5]] => ", a[a[5]]);

  a[6] = 1;
  b[0] = 10;

  
  write("\n\n\n");

  write("a[6]/2 => ", a[6]/2);
  write("b[0] => ", b[0]);
  write("b[a[6]/2] => ", b[a[6]/2]);

  write("\n");

  write("b[a[6]/2]-4 => ", b[a[6]/2]-4);
  write("a[6] => ", a[6]);

  write("\n");

  write("a[b[a[6]/2]-4] => ", a[b[a[6]/2]-4]);
  write("a[b[a[6]/2]-4]+a[6] => ", a[b[a[6]/2]-4]+a[6]);

  write("\n");
  write("a[2] => ", a[2]);
  write("a[a[b[a[6]/2]-4]+a[6]] => ", a[a[b[a[6]/2]-4]+1]);

}